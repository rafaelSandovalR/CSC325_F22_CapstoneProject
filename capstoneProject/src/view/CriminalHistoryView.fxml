<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="650.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18">
   <children>
      <VBox prefHeight="650.0" prefWidth="650.0">
         <children>
            <Button fx:id="backBtn" mnemonicParsing="false" onAction="#backToLoggedInPage" prefHeight="32.0" prefWidth="64.0" text="Back" />
            <Label alignment="TOP_CENTER" prefWidth="650.0" text="List Of Criminals">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Button mnemonicParsing="false" text="Edit">
               <VBox.margin>
                  <Insets bottom="20.0" left="300.0" top="20.0" />
               </VBox.margin>
            </Button>
            <TableView fx:id="criminalTable" prefHeight="200.0">
              <columns>
                <TableColumn prefWidth="100.0" text="ID" />
                <TableColumn prefWidth="125.0" text="First Name" />
                  <TableColumn prefWidth="125.0" text="Last Name" />
                  <TableColumn prefWidth="60.0" text="Age" />
                  <TableColumn prefWidth="50.0" text="Sex" />
                  <TableColumn prefWidth="90.0" text="Race" />
                  <TableColumn prefWidth="90.0" text="Hair Color" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
